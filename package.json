{
  "name": "hotel",
  "version": "0.3.4",
  "description": "Easily start, stop and access your servers from the browser",
  "main": "lib",
  "bin": "bin/index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "prepublish": "npm test",
    "test": "npm run build && npm run standard && mocha",
    "install": "node bin/install",
    "uninstall": "node bin/uninstall",
    "standard": " standard",
    "build": "babel src --out-dir lib --copy-files",
    "watch": "npm run build -- --watch",
    "daemon": "nodemon --exec babel-node -- src/daemon"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typicode/hotel.git"
  },
  "keywords": [
    "dev",
    "utility",
    "process",
    "manager",
    "local",
    "server"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typicode/hotel/issues"
  },
  "homepage": "https://github.com/typicode/hotel",
  "dependencies": {
    "debug": "^2.2.0",
    "express": "^4.12.3",
    "get-port": "^1.0.0",
    "got": "^2.9.2",
    "mkdirp": "^0.5.0",
    "once": "^1.3.2",
    "respawn-group": "^1.0.1",
    "socket.io": "^1.3.5",
    "sudo-block": "^1.2.0",
    "tildify": "^1.0.0",
    "unquote": "^1.1.0",
    "untildify": "^2.0.0",
    "update-notifier": "^0.5.0",
    "watch": "^0.16.0",
    "xtend": "^4.0.0",
    "yargs": "^3.8.0"
  },
  "devDependencies": {
    "babel": "^5.3.3",
    "mocha": "^2.2.1",
    "nodemon": "^1.3.7",
    "rimraf": "^2.3.3",
    "standard": "^4.3.1",
    "supertest": "^0.15.0"
  }
}
